# .env.sample
#
# Copy this file to .env and fill in your OAuth2 Proxy secrets.
# Do not commit the .env file to version control.

# Generate a random secret for the cookie secret.
# You can use the following command to generate a suitable secret:
# python -c 'import os, base64; print(base64.b64encode(os.urandom(32)).decode("utf-8"))'
OAUTH2_PROXY_COOKIE_SECRET=

# Get these values from your OAuth provider (e.g., Google, GitHub, etc.)
OAUTH2_PROXY_CLIENT_ID=
OAUTH2_PROXY_CLIENT_SECRET=
