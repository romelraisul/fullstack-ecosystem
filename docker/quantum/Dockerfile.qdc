# Quantum Deployment Center Dockerfile
FROM python:3.11-slim-bookworm
WORKDIR /app
COPY requirements.txt /app/requirements.txt
RUN pip install --no-cache-dir -r /app/requirements.txt && pip install --no-cache-dir prometheus-client fastapi uvicorn
COPY quantum_deployment_center.py /app/quantum_deployment_center.py
EXPOSE 5065
CMD ["uvicorn", "quantum_deployment_center:app", "--host", "0.0.0.0", "--port", "5065"]
