{
    "title": "QCAE â€” Quick View",
    "uid": "qcae-overview",
    "version": 1,
    "refresh": "10s",
    "time": {
        "from": "now-15m",
        "to": "now"
    },
    "links": [
        {
            "title": "Quantum + Ultra Overview",
            "url": "/d/quantum-ultra-overview",
            "targetBlank": false
        },
        {
            "title": "Numbers Now",
            "url": "/d/numbers-now",
            "targetBlank": false
        }
    ],
    "panels": [
        {
            "title": "QCAE jobs/s (5m)",
            "type": "stat",
            "targets": [
                {
                    "expr": "sum(rate(qcae_quantum_jobs_total[5m]))"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s"
                }
            },
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 6,
                "h": 5
            }
        },
        {
            "title": "QCAE p95 (5m)",
            "type": "stat",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(qcae_quantum_job_duration_seconds_bucket[5m])) by (le))"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.2
                            },
                            {
                                "color": "red",
                                "value": 0.5
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "x": 6,
                "y": 0,
                "w": 6,
                "h": 5
            }
        },
        {
            "title": "QCAE p99 (5m)",
            "type": "stat",
            "targets": [
                {
                    "expr": "histogram_quantile(0.99, sum(rate(qcae_quantum_job_duration_seconds_bucket[5m])) by (le))"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.4
                            },
                            {
                                "color": "red",
                                "value": 1
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "x": 12,
                "y": 0,
                "w": 6,
                "h": 5
            }
        },
        {
            "title": "Guardrail violations rps (5m)",
            "type": "stat",
            "targets": [
                {
                    "expr": "sum(rate(qcae_guardrail_violations_total[5m]))"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 0.005
                            },
                            {
                                "color": "red",
                                "value": 0.02
                            }
                        ]
                    }
                }
            },
            "gridPos": {
                "x": 18,
                "y": 0,
                "w": 6,
                "h": 5
            }
        },
        {
            "title": "QCAE jobs/s by kind (5m)",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum by (kind) (rate(qcae_quantum_jobs_total[5m]))",
                    "legendFormat": "{{kind}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "req/s"
                }
            },
            "gridPos": {
                "x": 0,
                "y": 5,
                "w": 24,
                "h": 8
            }
        },
        {
            "title": "QCAE p95 by kind (5m)",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum by (le, kind) (rate(qcae_quantum_job_duration_seconds_bucket[5m])))",
                    "legendFormat": "p95 {{kind}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            },
            "gridPos": {
                "x": 0,
                "y": 13,
                "w": 12,
                "h": 8
            }
        },
        {
            "title": "QCAE p99 by kind (5m)",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "max by (kind) (histogram_quantile(0.99, sum by (le, kind) (rate(qcae_quantum_job_duration_seconds_bucket[5m]))))",
                    "legendFormat": "p99 {{kind}} (max)"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            },
            "gridPos": {
                "x": 12,
                "y": 13,
                "w": 12,
                "h": 8
            }
        }
    ]
}
